{
  "presets": [
    [
      "es2015",
      {
        //        "modules": false
      }
    ],
    // compiler understands the native import syntax, and uses it for tree shaking
    "es2016",
    "es2017"
  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-object-rest-spread"
  ]
}